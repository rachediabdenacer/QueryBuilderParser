{
    "name": "rachediabdenacer/query-builder-parser",
    "homepage": "http://github.com/rachediabdenacer/QueryBuilderParser",
    "description": "Build complex Eloquent & QueryBuilder queries automatically when using jQuery-QueryBuilder",
    "keywords": [ "ajax", "eloquent", "jquery", "laravel", "parser", "query builder", "sql", "mongodb" ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Abdenacer Rachedi",
            "email": "rachedi.educ@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
          "RachediAbdenacer\\QueryBuilderParser\\": "src"
        }

    },
    "autoload-dev": {
        "psr-4": {
            "RachediAbdenacer\\QueryBuilderParser\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "illuminate/database": "^8.0||^7.0||^6.0||4.1 - 6.0",
        "ext-json": "*"
    },
    "suggest": {
        "jenssegers/mongodb": "Use QueryBuilderParser with MongoDB"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0||^8.5||^7.0",
        "mockery/mockery": "^1.3.1||^0.9.4"
    }
}
